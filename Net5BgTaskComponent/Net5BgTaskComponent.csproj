<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
      <TargetFramework>net5.0-windows10.0.19041.0</TargetFramework>
      <CsWinRTWindowsMetadata>10.0.19041.0</CsWinRTWindowsMetadata>
      <CsWinRTKeepGeneratedSources>true</CsWinRTKeepGeneratedSources>
      <CsWinRTComponent>true</CsWinRTComponent>
      <CsWinRTEnableLogging>true</CsWinRTEnableLogging>
      <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
      <AssemblyVersion>1.0.0.0</AssemblyVersion>
      <GeneratedFilesDir Condition="'$(GeneratedFilesDir)'==''">$([MSBuild]::NormalizeDirectory('$(MSBuildProjectDirectory)', '$(IntermediateOutputPath)', 'Generated Files'))</GeneratedFilesDir>
  </PropertyGroup>

    <ItemGroup>
        <FrameworkReference Update="Microsoft.Windows.SDK.NET.Ref" RuntimeFrameworkVersion="10.0.19041.13" />
        <FrameworkReference Update="Microsoft.Windows.SDK.NET.Ref" TargetingPackVersion="10.0.19041.13" />
    </ItemGroup>
        
  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.CsWinRT" Version="1.1.1" />
  </ItemGroup>
</Project>
